df = pd.read_csv("Unemployment in India.csv")
df = pd.read_csv("Unemployment_Rate_upto_11_2020.csv")
df.head()

"""**Statistics of Dataset**"""

df.describe()

"""**Structure of Dataset**"""

df.info()

"""**Checking if null values are present or not**"""

df.isna().sum()

"""**Correlation plot**"""

sns.heatmap(df.corr(), annot = True);

df.columns = ['Region','Date','Frequency','Estimated Unemployment Rate','Estimated Employed','Estimated Labour Participation Rate','Region.1','longitude','latitude']

plt.figure(figsize = (10,10))
plt.bar(df['Region.1'], df['Estimated Unemployment Rate'])
plt.xticks(rotation = 90)

"""**Interpretation : South region has the highest and west region has the lowest Estimated Unemployment Rate**"""

plt.figure(figsize = (12,10))
sns.catplot(x = 'Region',y = 'Estimated Unemployment Rate', data = df, kind = 'bar')
plt.xticks(rotation = 90)

"""**Interpretation : Among all the states Haryana has the highest and Meghalaya has the lowest EUR**"""

plt.figure(figsize = (12,10))
plt.title('India\'s Unemployment')
sns.histplot(x = 'Estimated Unemployment Rate',hue = 'Region.1',data = df)
plt.show()
